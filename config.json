{
  "prefix": "tmux send '",
  "suffix": "' c-M",
  "osc_messages": {
    "/numbers": {
      "command": "say numbers $1",
      "trigger_type": "continuous"
    },
    "/pulse": {
      "command_rising": "say rising",
      "command_falling": "say falling",
      "trigger_type": "threshold",
      "threshold": 0.35
    }
  }
}
