{
  "ip": "127.0.0.1",
  "port": 8000,
  "prefix": "tmux send '",
  "suffix": "' c-M",
  "osc_messages": {
    "/numbers": {
      "command": "say numbers $1",
      "trigger_type": "continuous"
    },
    "/pulse": {
      "command_rising": "effect give @a minecraft:night_vision 9999 1 true",
      "command_falling": "effect clear @a minecraft:night_vision",
      "trigger_type": "threshold",
      "threshold": 0.35
    }
  }
}
